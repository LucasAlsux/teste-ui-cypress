pipeline {
    agent any

    stages {
        stage('start') {
            steps {
            
             git branch: 'main', url: 'https://github.com/LucasAlsux/teste-ui-cypress.git'
             bat 'npm install'
            }
        }
        
        stage('Subir servidor') {
            steps {
            bat 'start /b npx serverest'
            }

        }
        
        stage('test') {
            steps {
            bat 'set NO_COLOR=1'
            bat 'npm test'
            }
        }
    }
}
